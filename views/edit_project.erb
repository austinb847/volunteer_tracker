<div class="jumbotron">
  <h1>Update <%= @project.title%></h1>
</div>
<form action="/projects/<%= @project.id %>" method="post">
  <input name="_method" type="hidden" value="patch">
    
  <label for="project_title">Change Project Title</label>
  <input id="project_title" name="project_title" type="text" class="form-control" value= "<%= @project.title()%>" >
  <br>
  <button type="submit" class="btn btn-primary">Update Project</button>
</form>
<br>
<p><a href="/projects">Return to projects</a></p>